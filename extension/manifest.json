{
  "manifest_version": 3,
  "name": "Account Manager",
  "version": "1.0.0",
  "description": "Manage accounts and cookies",
  "permissions": [
    "cookies",
    "storage",
    "proxy",
    "webRequest"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  }
}
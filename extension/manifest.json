{
  "manifest_version": 3,
  "name": "Profile & Cookie Manager",
  "version": "1.0.0",
  "description": "Manage browser profiles, cookies, and proxies",
  "permissions": [
    "cookies",
    "storage",
    "proxy",
    "webRequest",
    "webRequestBlocking"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  }
}